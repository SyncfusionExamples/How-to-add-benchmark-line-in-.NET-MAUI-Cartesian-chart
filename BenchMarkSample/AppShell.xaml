<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BenchMarkSample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BenchMarkSample"
    Shell.FlyoutBehavior="Flyout"
    Title="BenchMarkSample">

    <ShellContent
               Title="Horizontal Line Annotation" ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>
    <ShellContent Title="Custom Drawing" ContentTemplate="{DataTemplate local:CustomDrawing}" 
                  Route="customDrawing"/>

</Shell>
